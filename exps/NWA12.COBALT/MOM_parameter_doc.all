KV_BBL_MIN = 1.0E-06            !   [m2 s-1] default = 1.0E-06
                                ! The minimum viscosities in the bottom boundary layer.
KV_TBL_MIN = 1.0E-06            !   [m2 s-1] default = 1.0E-06
